<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ncepu.yun.mapper.UserMapper">
    <select id="selectUserByName" resultType="com.ncepu.yun.entity.User">
      select * from ly_user where id = #{id}
    </select>

    <select id="selectAllUser" resultType="com.ncepu.yun.entity.User">
        select userName, accountName, description from ly_user;
    </select>
</mapper>
